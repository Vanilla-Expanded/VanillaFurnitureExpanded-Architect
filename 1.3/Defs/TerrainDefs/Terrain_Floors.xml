  <!--====================================== Cobblestone floor =======================================-->
  
  <TerrainDef Abstract="True" ParentName="FloorBase" Name="CobblestoneBase">
    <renderPrecedence>220</renderPrecedence>
    <description>Tiles made out of chipped stones. These are not beautiful, but they make good surfaces for roads and outdoor walkways. Deconstructing cobblestone flooring yields no resources.</description>
    <texturePath>Terrains/Cobblestone</texturePath>
    <resourcesFractionWhenDeconstructed>0</resourcesFractionWhenDeconstructed>
    <statBases>
      <WorkToBuild>400</WorkToBuild>
      <Beauty>0</Beauty>
    </statBases>
    <designatorDropdown>VFEArch_CobblestoneFloor</designatorDropdown>
    <tags>
      <li>Road</li>
    </tags>
  </TerrainDef>

